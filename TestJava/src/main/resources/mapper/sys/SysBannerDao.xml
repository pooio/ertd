<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.metaShare.modules.sys.dao.SysBannerDao">
    <select id="getList" parameterType="com.metaShare.modules.sys.entity.SysBanner" resultType="hashmap">
        SELECT *
        from sys_banner
        where DELETED =0
    </select>
</mapper>
